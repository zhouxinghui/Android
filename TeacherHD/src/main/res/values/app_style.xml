<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--扶뒬못start-->
    <style name="home_navigation">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/home_navigation_background</item>
        <item name="android:textColor">@color/home_navigation_text_color</item>
        <item name="android:paddingTop">@dimen/y15</item>
        <item name="android:paddingBottom">@dimen/y15</item>
        <item name="android:drawablePadding">@dimen/y10</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="home_tv_btn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">@dimen/x20</item>
        <item name="android:background">@drawable/home_tv_bg</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:drawablePadding">@dimen/x20</item>
    </style>
    <!--扶뒬못end-->
</resources>